                      <Parameter AllowDbNull="false" AutogeneratedName="FuelReportCode" ColumnName="ID" DataSourceName="VoyageCost.dbo.RPMInfo" DataTypeServer="int" DbType="Int32" Direction="Input" ParameterName="@FuelReportCode" Precision="0" ProviderType="Int" Scale="0" Size="4" SourceColumn="FuelReportCode" SourceColumnNullMapping="false" SourceVersion="Current" />
