<access-policy>
    <cross-domain-access>
        <policy>
            <allow-from http-methods="*" http-request-headers="*" >
                <domain uri="*" />
            </allow-from>
            <grant-to>
                <resource path="/" include-subpaths="true"/>
            </grant-to>
        </policy>
    </cross-domain-access>
</access-policy>